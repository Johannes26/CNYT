    "## 15. Matriz transpuesta"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Mat_Trans(M1):\n",
    "    M2=[]\n",
    "    for i in range(len(M1[0])):\n",
    "        fila=[]\n",
    "        for j in range(len(M1)):\n",
    "            fila.append(M1[j][i])\n",
    "        M2.append(fila)\n",
    "    return M2"
   ]
  },
  {
   "attachments": {
    "image.png": {
     "image/png": ""
    }
   },
   "cell_type": "markdown",
   "metadata": {},
   "source": [
