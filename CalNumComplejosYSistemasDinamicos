    "## 24. ¬øEs matriz hermitiana?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 63,
   "metadata": {},
   "outputs": [],
   "source": [
    "def hermitiana(M1):\n",
    "    res=True\n",
    "    M1=matriz_Adj(M1)\n",
    "    daga=matriz_Adj(M1)\n",
    "    a=matriz_Adj(M1)\n",
    "    for i in range(len(M1)):\n",
    "        for j in range(len(M1[0])):\n",
    "            if daga[i][j][0]!=M1[i][j][0] or daga[i][j][1]!=M1[i][j][1]:\n",
    "                res=False\n",
    "    return res"
   ]
  },
