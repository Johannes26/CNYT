    "## 20. Accion de una matriz sobre un vector"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 51,
   "metadata": {
    "scrolled": true
   },
   "outputs": [],
   "source": [
    "def accion(M1,V):\n",
    "    if(len(M1[0])==len(V)):\n",
    "        vector=[]\n",
    "        for i in range(len(M1)):\n",
    "            result=(0,0)\n",
    "            for l in range(len(M1[i])):\n",
    "                result=Sumar_Comp(result,Multi_Comp(M1[i][l],V[l]))\n",
    "            vector.append(result)\n",
    "        \n",
    "        return vector\n",
    "    else:\n",
    "        print(\"Error no son compatibles\")"
   ]
  },
