   "source": [
    "def EnsambleSistemaYDinamicas(M1,M2,V1,V2,t):\n",
    "    if(ValidarSistema(M1,V1) and ValidarSistema(M2,V2)):\n",
    "        Mdinamica=tensor(M1,M2)\n",
    "        Vectorini=tensorvectores(V1,V2)\n",
    "        for x in range(t):            \n",
    "            Vectorini=accion(Mdinamica,Vectorini)\n",
    "        labels=[]\n",
    "        estado=[]\n",
    "        for x in range(len(Vectorini)):\n",
    "            labels.append('Pto.'+str(x))\n",
    "            estado.append(Vectorini[x][0])\n",
    "        index = np.arange(len(labels))\n",
    "        plt.bar(index, estado)\n",
    "        plt.xlabel('Estado')\n",
    "        plt.ylabel('Valor')\n",
    "        plt.xticks(index, labels, rotation=30)\n",
    "        plt.title('Evoluci√≥n din√°mica del sistema')\n",
    "        plt.show()\n",
    "        return Vectorini\n",
    "    else:\n",
    "        print(\"Sistemas que ingreso no cumplen\")"
   ]
  },
