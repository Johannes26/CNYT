    "## 19. Producto entre matrices"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "metadata": {},
   "outputs": [],
   "source": [
    "def prod_Mat(M1,M2):\n",
    "    if(len(M1[0])==len(M2)):\n",
    "        M2=Mat_Trans(M2)\n",
    "        matriz=[]\n",
    "        for i in range(len(M1)):\n",
    "            fila=[]\n",
    "            for j in range(len(M2)):\n",
    "                result=(0,0)\n",
    "                for l in range(len(M1[i])):\n",
    "                    result=Sumar_Comp(result,Multi_Comp(M1[i][l],M2[j][l]))\n",
    "                fila.append(result)\n",
    "            matriz.append(fila)\n",
    "        \n",
    "        return matriz\n",
    "    else:\n",
    "        print(\"Error no son compatibles\")\n",
    "            "
   ]
  },
  {
