    "## 12. Adici√≥n de matrices complejos"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 27,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Suma_Matrices(M1,M2):\n",
    "    if len(M1)==len(M2) and len(M1[0])==len(M2[0]):\n",
    "        suma=[]\n",
    "        for i in range(len(M1)):\n",
    "            suma.append(Suma_Vec(M1[i],M2[i]))\n",
    "        return suma\n",
    "    else:\n",
    "        print(\"No se puede sumar matrices de diferentes dimensiones\")\n",
    "        "
   ]
  },
  {
