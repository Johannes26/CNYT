   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Conjugado_Comp(C1):\n",
    "    Real= C1[0]\n",
    "    Comp=-C1[1]\n",
    "    \n",
    "    return((Real,Comp))"
   ]
  },
