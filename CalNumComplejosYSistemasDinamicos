    "#  3. Resta de numeros complejos"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "def resta_Comp(C1,C2):\n",
    "    Real=C1[0]-C2[0]\n",
    "    Comp=C1[1]-C2[1]\n",
    "    \n",
    "    return((Real,Comp))"
   ]
  },
