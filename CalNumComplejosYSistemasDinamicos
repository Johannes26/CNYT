    "## 23. ¬øEs matriz unitaria?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 60,
   "metadata": {},
   "outputs": [],
   "source": [
    "def unitaria(M1):\n",
    "    M1=matriz_Adj(M1)\n",
    "    daga=matriz_Adj(M1)\n",
    "    a=matriz_Adj(M1)\n",
    "    cont=0\n",
    "    res=True\n",
    "    mult=prod_Mat(M1,daga)\n",
    "    for i in range(len(M1)):\n",
    "        for j in range(len(M1[0])):\n",
    "            if round(mult[i][j][0])==1:\n",
    "                cont+=1\n",
    "    if cont!=len(M1):\n",
    "        res=False\n",
    "    \n",
    "    return res\n"
   ]
  },
  {
   "attachments": {
    "image.png": {
     "image/png": ""
    }
   },
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Captura de pantalla 2019-09-08 a la(s) 11.09.16 p. m.![image.png](attachment:image.png)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 61,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "True"
      ]
     },
     "execution_count": 61,
