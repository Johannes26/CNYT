    "## 4. Division"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Division_Comp(C1,C2):\n",
    "    if C2==(0,0): \n",
    "        print(\"No se puede dividir entre 0\")\n",
    "    else:\n",
    "        #Conjugado\n",
    "        Conj=(C2[0],-C2[1])\n",
    "\n",
    "        #Multiplicacion Numerador\n",
    "        Denom=Multi_Comp(Conj,C2)\n",
    "\n",
    "        #Multiplicacion Denominador\n",
    "        Num=Multi_Comp(C1,Conj)\n",
    "\n",
    "        return(Num,Denom)"
   ]
  },
  {
   "attachments": {
    "image.png": {
     "image/png": ""
    }
   },
