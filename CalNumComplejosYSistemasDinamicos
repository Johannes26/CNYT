   "source": [
    "## 24. Producto tensor"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 66,
   "metadata": {},
   "outputs": [],
   "source": [
    "def tensor(M1,M2):\n",
    "    matriz=[]\n",
    "    m=len(M1)\n",
    "    n=len(M1[0])\n",
    "    p=len(M2)\n",
    "    q=len(M2[0])\n",
    "    for i in range(m*p):\n",
    "        fila=[]\n",
    "        for j in range(n*q):\n",
    "            fila.append(Multi_Comp(M1[i//p][j//q],M2[i%p][j%q]))\n",
    "        matriz.append(fila)\n",
    "    return matriz"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "# 24. Producto tensor vectores"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 67,
   "metadata": {},
   "outputs": [],
   "source": [
    "def tensorvectores(V1,V2):\n",
    "    fila=[]\n",
    "    for j in range(len(V1)*len(V2)):\n",
    "        fila.append(Multi_Comp(V1[j//len(V2)],V2[j%len(V2)]))\n",
    "    return fila"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 68,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
