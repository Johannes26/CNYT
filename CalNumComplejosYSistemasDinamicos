    "## 7. Forma rectangular a polar"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 14,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Rec_Polar(C1):\n",
    "    p = Modulo_Comp(C1)\n",
    "    T = math.atan2(C1[1],C1[0])* 180 / math.pi\n",
    "    res = [p, T]\n",
    "    return res"
   ]
  },
  {
