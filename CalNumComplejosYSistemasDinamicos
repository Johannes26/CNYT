    "## 10. inversa de un vector"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Inv_Vec(v1):\n",
    "    v2=[]\n",
    "    for i in range(len(v1)):\n",
    "        v2.append((v1[i][0]*-1,v1[i][1]*-1))\n",
    "    return v2\n",
    "        "
   ]
  },
