    "## 16. Matriz conjugada"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
   "metadata": {},
   "outputs": [],
   "source": [
    "def Matriz_Conj(M1):\n",
    "    for i in range(len(M1)):\n",
    "        for j in range(len(M1[0])):\n",
    "            M1[i][j]=Conjugado_Comp(M1[i][j])\n",
    "        \n",
    "    return M1"
   ]
  },
