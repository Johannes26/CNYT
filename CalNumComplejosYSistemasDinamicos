  {
   "cell_type": "code",
   "execution_count": 216,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Importaci√≥n de librer√≠as\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 217,
   "metadata": {},
   "outputs": [],
   "source": [
    "def ValidarSistema(M,V):\n",
    "    res=True\n",
    "    for j in range(len(M[0])):\n",
    "        suma=0\n",
    "        for i in range(len(M)):\n",
    "            suma+=M[i][j][0]\n",
    "        res=res and suma==1\n",
    "        \n",
    "    suma=0\n",
    "    for j in range(len(V)):\n",
    "        suma+=V[j][0]\n",
    "    return (res and suma==1)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 218,
   "metadata": {},
   "outputs": [],
   "source": [
    "def SistemaYDinamica(dinamica,M,V,t):\n",
    "        \n",
    "    if(dinamica!=0 and dinamica!=1 and dinamica !=2):\n",
    "        print(\"dinamica que inserto no es correcta\")\n",
    "    else:\n",
    "        if((dinamica==0 or dinamica==1) and ValidarSistema(M,V)):\n",
    "            for x in range(t):\n",
    "                V=accion(M,V)\n",
    "        elif(dinamica==2 and unitaria(M)):\n",
    "            matrizModulos=[]\n",
    "            for x in range(len(M)):\n",
    "                fila=[]\n",
    "                for y in range(len(M[0])):\n",
    "                    fila.append(Multi_Comp(M[x][y],Conjugado_Comp(M[x][y])))\n",
    "                matrizModulos.append(fila)\n",
    "            suma=0\n",
    "            vec=[]\n",
    "            for i in range(len(V)):\n",
    "                vec.append(Multi_Comp(V[i],Conjugado_Comp(V[i])))\n",
    "                suma+=vec[i][0]\n",
    "            if(suma!=1):\n",
    "                print(\"El vector inicial no es correcto\")\n",
    "            else:\n",
    "                for x in range(t):\n",
    "                    V=accion(matrizModulos,vec)\n",
    "        if(V):\n",
    "            labels=[]\n",
    "            estado=[]\n",
    "            for x in range(len(V)):\n",
    "                labels.append('Pto.'+str(x))\n",
    "                estado.append(V[x][0])\n",
    "\n",
    "            index = np.arange(len(labels))\n",
    "            plt.bar(index, estado)\n",
    "            plt.xlabel('Estado')\n",
    "            plt.ylabel('Valor')\n",
    "            plt.xticks(index, labels, rotation=30)\n",
    "            plt.title('Evoluci√≥n din√°mica del sistema')\n",
    "            plt.show()\n",
    "            return V\n",
    "    ##else:\n",
    "      ##  print(\"No es una matriz correspondiente a la dinamica del sistema\")"
   ]
  },
