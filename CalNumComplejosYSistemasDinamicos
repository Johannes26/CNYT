   "metadata": {},
   "outputs": [],
   "source": [
    "def Suma_Vec(v1,v2):\n",
    "    suma=[]\n",
    "    for i in range(len(v1)):\n",
    "        suma.append(Sumar_Comp(v1[i],v2[i]))\n",
    "    return suma\n",
    "        "
   ]
